<div class="content">
	<div class="banner">
		<div class="container">
			<h1>《<%- page.category %>》的目录</h1>
		</div>
	</div>
	<div class="container">
		<ul class="catalogue">
			<% page.posts.each(function (page) { %>
			<li>
				<a href="<%- url_for(page.path) %>">
					<span class="title"><%= page.title %></span>
					<span class="date"><span><%- date(page.date, "YYYY-MM-DD") %></span></span>
				</a>
			</li>
			<% }) %>
		</ul>
	</div>
</div>